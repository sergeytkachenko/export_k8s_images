{
  "name": "export_k8s_images",
  "version": "0.0.1",
  "description": "Export Kubernetes container images to a file",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "bin": {
    "export-k8s-images": "./dist/cli.js",
    "export-k8s-images-offline": "./dist/cli-offline.js",
    "export-compose-images-offline": "./dist/cli-compose-offline.js"
  },
  "config": {
    "defaultRelease": "enrichment"
  },
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "export": "ts-node src/cli.ts",
    "export:prod": "node dist/cli.js",
    "export:release": "ts-node src/cli.ts -r $npm_package_config_defaultRelease",
    "export:release:prod": "node dist/cli.js -r $npm_package_config_defaultRelease",
    "export:offline": "ts-node src/cli-offline.ts",
    "export:offline:prod": "node dist/cli-offline.js",
    "export:offline:release": "ts-node src/cli-offline.ts -r $npm_package_config_defaultRelease",
    "export:offline:release:prod": "node dist/cli-offline.js -r $npm_package_config_defaultRelease",
    "export:compose": "ts-node src/cli-compose-offline.ts",
    "export:compose:prod": "node dist/cli-compose-offline.js",
    "export:compose:build": "ts-node src/cli-compose-offline.ts -b",
    "export:compose:build:prod": "node dist/cli-compose-offline.js -b"
  },
  "dependencies": {
    "@nestjs/common": "^11.0.1",
    "@nestjs/core": "^11.0.1",
    "@nestjs/platform-express": "^11.0.1",
    "js-yaml": "^4.1.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@types/express": "^5.0.0",
    "@types/jest": "^30.0.0",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.10.7",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "jest": "^30.0.0",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
